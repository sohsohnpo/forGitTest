<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>TAC_InputCheckMobilePhone</fullName>
    <active>true</active>
    <description>半角数字で入力されているか確認する
対象項目：携帯電話</description>
    <errorConditionFormula>NOT(REGEX( TAC_MobilePhone__c , &quot;^[0-9]*$&quot; ))</errorConditionFormula>
    <errorDisplayField>TAC_MobilePhone__c</errorDisplayField>
    <errorMessage>半角数字で入力してください。</errorMessage>
</ValidationRule>
