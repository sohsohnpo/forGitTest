<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>TAC_InputCheckPhone</fullName>
    <active>true</active>
    <description>半角数字で入力されているか確認する
対象項目：電話（記入例：0312345678）</description>
    <errorConditionFormula>NOT(REGEX( Phone , &quot;^[0-9]*$&quot; ))</errorConditionFormula>
    <errorDisplayField>Phone</errorDisplayField>
    <errorMessage>半角数字で入力してください。</errorMessage>
</ValidationRule>
